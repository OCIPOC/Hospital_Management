            <th>Patient ID</th>
            <th>Patient Name</th>
            <th>First Symptoms</th>
            <th>Doctor</th>
        </tr>
    </thead>
    <tbody>
        {{#each reception_data}}
        <tr>
            <td scope="row">{{this.MAKB}} </td>
            <td>{{this.NGAYKB}}</td>
            <td>{{this.MABN}}</td>
