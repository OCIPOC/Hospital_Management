<div class="card border-light shadow-sm mb-4">
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-centered table-nowrap mb-0 rounded table-hover">


                <thead class="thead-light">
                    <tr>
                        <th class="border-0">USER_ID</th>
                        <th class="border-0">USERNAME</th>
                        <th class="border-0">PASSWORD</th>
                        <th class="border-0">ACCOUNT_STATUS</th>
                        <th class="border-0">LOCK_DATE</th>
                        <th class="border-0">EXPIRY_DATE</th>
                        <th class="border-0">DEFAULT_TABLESPACE</th>
                        <th class="border-0">CREATED</th>
                        <th class="border-0">PROFILE</th>
                    </tr>
                </thead>
                <tbody>
                    <!-- Item -->

                    {{#each users }}
                    <tr>
                        <td class="border-0"><a href="#" class="text-primary font-weight-bold">
                                {{this.USER_ID}}
                            </a>
                        </td>

                        <td class="border-0">
                            {{this.USERNAME}}
                        </td>
                        <td class="border-0">
                            {{ this.PASSWORD}}
                        </td>

                        <td class="border-0">
                            {{this.ACCOUNT_STATUS}}
                        </td>


                        <td class="border-0">
                            {{this.LOCK_DATE}}
                        </td>


                        <td class="border-0">
                            {{ this.EXPIRY_DATE}}
                        </td>

                        <td class="border-0">
                            {{this.DEFAULT_TABLESPACE}}
                        </td>


                        <td class="border-0">
                            {{this.CREATED}}
                        </td>


                        <td class="border-0">
                            {{this.PROFILE}}
                        </td>





                    </tr>
                    {{/each}}



                </tbody>
            </table>
        </div>
    </div>
</div>