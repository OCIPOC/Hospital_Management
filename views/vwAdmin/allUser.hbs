{{#section 'css'}}
<style>
    .w-br {

        word-wrap: break-word;

    }
</style>
{{/section}}


<div class="card border-light shadow-sm mb-4">
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-centered table-nowrap mb-0 rounded table-hover">


                <thead class="thead-light">
                    <tr>
                        <th class="border-0 w-br ">USER_ID</th>
                        <th class="border-0 w-br">USERNAME</th>
                        <th class="border-0 w-br">PASSWORD</th>
                        <th class="border-0 w-br">ACCOUNT_STATUS</th>
                        <th class="border-0 w-br">LOCK_DATE</th>
                        <th class="border-0 w-br">EXPIRY_DATE</th>
                        <th class="border-0 w-br">DEFAULT_TABLESPACE</th>
                        <th class="border-0 w-br">CREATED</th>
                        <th class="border-0 w-br">PROFILE</th>
                    </tr>
                </thead>
                <tbody>
                    <!-- Item -->

                    {{#each users }}
                    <tr>
                        <td class="border-0 w-br"><a href="#" class="text-primary font-weight-bold">
                                {{this.USER_ID}}
                            </a>
                        </td>

                        <td class="border-0 w-br">
                            {{this.USERNAME}}
                        </td>
                        <td class="border-0 w-br">
                            {{ this.PASSWORD}}
                        </td>

                        <td class="border-0 w-br">
                            {{this.ACCOUNT_STATUS}}
                        </td>


                        <td class="border-0 w-br">
                            {{this.LOCK_DATE}}
                        </td>


                        <td class="border-0 w-br">
                            {{ this.EXPIRY_DATE}}
                        </td>

                        <td class="border-0 w-br">
                            {{this.DEFAULT_TABLESPACE}}
                        </td>


                        <td class="border-0 w-br">
                            {{this.CREATED}}
                        </td>


                        <td class="border-0 w-br">
                            {{this.PROFILE}}
                        </td>





                    </tr>
                    {{/each}}



                </tbody>
            </table>
        </div>
    </div>
</div>